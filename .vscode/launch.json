{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "name": "Launch via yarn",
      "request": "launch",
      "runtimeArgs": ["start"],
      "runtimeExecutable": "yarn",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "cwd": "${workspaceRoot}",
      "console": "integratedTerminal"
    }
  ]
}
